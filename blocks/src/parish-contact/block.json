{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "fim-parish-info/parish-contact",
	"version": "0.1.0",
	"title": "Parish Contact Information",
	"category": "fim-parish-info",
	"description": "Displays the parish contact information",
	"supports": {
		"html": false
	},
	"attributes":{
		"show_address": {
			"type" : "boolean",
			"default" : true
		},
		"show_phone": {
			"type" : "boolean",
			"default" : true
		},
		"show_office_hours": {
			"type" : "boolean",
			"default": false
		},
		"show_map": {
			"type" : "boolean",
			"default" : true
		},
		"hide_headings":{
			"type" : "boolean",
			"default" : false
		},
		"className": {
			"type": "string",
			"default": "wp-blocks-fim-parish-info-parish-contact"
		}
	},
	"textdomain": "fim-parish-info",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
